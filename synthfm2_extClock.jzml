<SCRIPT name="StepSeqScript(MIDI_ARGS)" script="decl i,k,j;
decl vector = 0;

if (counter!=precCounter&&Container.Transport.StartStop.x==1) {

Leds.value = 0;
j = counter%16;
for (k=0;k<=12;k++) {
	if (Pitch.x[(counter%16)+(k*16)] == 1) {
